@go.Package { name = "mikea/declix/content" }
abstract module mikea.declix.Content

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.8.0#/go.pkl" 

typealias Content = String | File | Url | Base64 | Render | Hashed;

class File {
    file: String
    sha256: String?
}

class Url {
    url: String
    sha256: String?
}

class Base64 {
    base64: String
    sha256: String?
}

class Hashed {
    sha256: String
}

function Resource(r: Resource): Content = new Base64 {
    base64 = r.base64
    sha256 = r.sha256
}

abstract class Render {
    abstract function render(): String

    _result: String = render()
    _sha256: String = _result.sha256
}