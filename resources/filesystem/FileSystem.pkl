@go.Package { name = "mikea/declix/resources/filesystem" }
module mikea.declix.resources.FileSystem

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.7.0#/go.pkl" 
import "../Resources.pkl"
import "../../content/Content.pkl"


class File extends Resources.Resource  {
    type: "file"
    path: String
    state: State
}

typealias State = Present | Missing

class Present {
    content: Content.Content
    owner: String
    group: String
    permissions: String
}

class Missing {

}